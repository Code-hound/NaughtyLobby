<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-sm-6">
        <%= simple_form_for(@user) do |f| %>
          <%= f.input :photo_url, label: "Picture Url" %>
          <%= f.input :photo_url_cache, as: :hidden %>
          <%= f.button :submit, "Upload picture" %>
        <% end %>
      </div>
    </div>
  </div>
</div>
